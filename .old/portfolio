<script>
    document.addEventListener('DOMContentLoaded', function () {
  
      gsap.registerPlugin(ScrollTrigger);
  
      const mm = gsap.matchMedia();
  
      mm.add("(min-width: 992px)", () => {
        const leaderInfos = document.querySelectorAll('.waveAnimate');
        leaderInfos.forEach((leader, index) => {
          gsap.from(leader, {
            opacity: 0,
            y: 50,
            duration: 0.5,
            ease: "power3.out",
            scrollTrigger: {
              trigger: leader,
              start: "top 80%",
              toggleActions: "play none none none",
            },
            delay: index * 0.2
          });
        });
      });
      
    });
</script>

<script>
    $(document).ready(function() {
        // Close navbar when clicking outside
        $(document).click(function(event) {
            var clickover = $(event.target);
            var _opened = $(".navbar-collapse").hasClass("show");
            if (_opened === true && !clickover.hasClass("navbar-toggler") && !clickover.closest('.navbar').length) {
                $(".navbar-toggler").click();
            }
        });

        // Close navbar on scroll
        $(window).scroll(function() {
            var _opened = $(".navbar-collapse").hasClass("show");
            if (_opened === true) {
                $(".navbar-toggler").click();
            }
        });
    });
    </script>

<script>
    document.addEventListener('DOMContentLoaded', function () {
      gsap.registerPlugin(ScrollTrigger);
  
      const mm = gsap.matchMedia();
  
      mm.add("(min-width: 992px)", () => {
        gsap.utils.toArray('.scrollAnimate').forEach((element) => {
          gsap.from(element, {
            opacity: 0,
            scale: 0.5, 
            duration: 1,
            ease: 'power2.out',
            scrollTrigger: {
              trigger: element,
              start: 'top 80%', 
              end: 'bottom 60%', 
              toggleActions: 'play none none none',
              scrub: 1,
            },
          });
        });
      });
    });
</script>

